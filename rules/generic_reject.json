{
    "GenericRejectButton": {
        "detectors": [
            {
                "presentMatcher": {
                    "type": "css",
                    "target": {
                        "selector": "[class*='cookie' i], [id*='cookie' i], [class*='consent' i], [id*='consent' i]",
                        "displayFilter": true
                    }
                },
                "showingMatcher": {
                    "type": "css",
                    "target": {
                        "selector": "button, [role='button'], a, div",
                        "textFilter": ["Reject all", "Decline all", "Reject cookies", "Decline cookies", "Reject", "Decline", "Necessary only", "Only essential"],
                        "displayFilter": true
                    }
                }
            }
        ],
        "methods": [
            {
                "name": "HIDE_CMP"
            },
            {
                "name": "DO_CONSENT",
                "action": {
                    "type": "click",
                    "target": {
                        "selector": "button, [role='button'], a, div",
                        "textFilter": ["Reject all", "Decline all", "Reject cookies", "Decline cookies", "Reject", "Decline", "Necessary only", "Only essential"]
                    }
                }
            }
        ]
    }
}
